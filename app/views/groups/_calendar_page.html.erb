
<%= month_calendar() do |date| %>
  <div style="display: flex;justify-content:space-between">
    <%= date %>
  </div>
  <% @members.each do |member| %>
      <% if member.date_of_birth.month == date.month && member.date_of_birth.day == date.day%>
        <div>
          <a href = "#" ><%= member.last_name + ' ' + member.first_name%></a>
        </div>
      <%end%>

    </div>
  <% end %>
<% end %>
